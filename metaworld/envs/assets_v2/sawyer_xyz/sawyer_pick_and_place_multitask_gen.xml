        <texture file="../../franka_sim/objects/textures/pepsi_can.png" name="tex-pepsi" type="2d"/>
        <material name="pepsi" reflectance="0.7" texrepeat="5 5" texture="tex-pepsi" texuniform="true"/>

        <mesh file="../../franka_sim/objects/cereal.msh" name="cereal_mesh"/>
        <texture type="2d" file="../../franka_sim/objects/textures/cereal.png" rgb1="1 1 1" name="tex-cereal"/>
        <material name="cereal" reflectance="0.5" texrepeat="1 1" texture="tex-cereal" texuniform="false"/>

        <mesh file="../../franka_sim/objects/lemon.msh" name="lemon_mesh" scale="1.5 1 1"/>
        <texture file="../../franka_sim/objects/textures/lemon.png" type="2d" name="tex-lemon"/>
        <material name="lemon" reflectance="0.7" texrepeat="60 50" texture="tex-lemon" texuniform="true"/>

        <mesh file="../../franka_sim/objects/milk.msh" name="milk_mesh" scale="0.9 0.9 0.9"/>
